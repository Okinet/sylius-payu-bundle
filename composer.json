{
    "name": "fsi/sylius-payu-bundle",
    "type": "symfony-bundle",
    "description": "PayU payments integration for Symfony2 e-commerce applications.",
    "keywords": ["Sylius", "PayU", "payments"],
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "doctrine/doctrine-bundle": "~1.3@dev",
        "sylius/payum-bundle": "1.0.*@dev",
        "sylius/payments-bundle": "1.0.*@dev",
        "sylius/resource-bundle": "1.0.*@dev",
        "sylius/order-bundle": "1.0.*@dev",
        "sylius/money-bundle": "1.0.*@dev"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "FSi\\Bundle\\SyliusPayuBundle\\": "" }
    },
    "target-dir": "FSi/Bundle/SyliusPayuBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}